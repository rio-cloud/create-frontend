{
    "name": "CREATE_RIO_FRONTEND_appName",
    "description": "A frontend based on @rio-cloud/create-frontend",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:ci": "vitest run --reporter=junit --outputFile.junit=./results/junit.xml",
        "test-dev": "vitest",
        "coverage": "vitest run --coverage",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
        "lint-fix": "npm run lint -- --fix",
        "audit": "npm audit --registry https://registry.npmjs.org --audit-level moderate --parseable --omit=dev",
        "format-code": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format-code-on-commit": "npm run format-code && git add",
        "pull-translations": "phrase pull --access_token $PHRASEAPP_ACCESS_TOKEN && node mergeTranslations.js",
        "cypress-open": "cypress open",
        "cypress-run": "cypress run",
        "cypress-ci": "start-server-and-test start http://127.0.0.1:3000 cypress-run"
    },
    "dependencies": {
        "@redux-devtools/extension": "3.3.0",
        "@reduxjs/toolkit": "2.2.6",
        "@rio-cloud/rio-notifications-component": "0.2.5",
        "@rio-cloud/rio-session-expired-info": "1.0.7",
        "@rio-cloud/rio-uikit": "1.2.2",
        "@rio-cloud/rio-user-menu-component": "3.0.0",
        "@sentry/browser": "8.19.0",
        "events": "3.3.0",
        "iframe-resizer-react": "1.1.0",
        "lodash": "4.17.21",
        "oidc-client-ts": "3.0.1",
        "qs": "6.12.3",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-hook-form": "7.52.1",
        "react-intl": "6.6.8",
        "react-redux": "9.1.2",
        "react-router": "6.25.1",
        "react-router-dom": "6.25.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "6.4.7",
        "@testing-library/react": "15.0.7",
        "@testing-library/user-event": "14.5.2",
        "@types/lodash": "4.17.7",
        "@types/node": "20.14.11",
        "@types/qs": "6.9.15",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@types/react-redux": "7.1.33",
        "@typescript-eslint/eslint-plugin": "7.17.0",
        "@typescript-eslint/parser": "7.17.0",
        "@vitejs/plugin-react": "4.3.1",
        "cypress": "13.13.1",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-compat": "4.2.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jest": "27.9.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-react": "7.35.0",
        "eslint-plugin-react-hooks": "4.6.2",
        "eslint-plugin-testing-library": "6.2.2",
        "jsdom": "24.1.1",
        "msw": "2.3.2",
        "pre-commit": "1.2.2",
        "prettier": "3.3.3",
        "rollup-plugin-visualizer": "5.12.0",
        "start-server-and-test": "2.0.4",
        "typescript": "5.5.4",
        "vite": "5.3.4",
        "vitest": "2.0.4",
        "vitest-fetch-mock": "0.3.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "pre-commit": [
        "format-code-on-commit"
    ],
    "msw": {
        "workerDirectory": "public"
    }
}
